                "0": {
                  "name": "entity.name.tag.monkyaml"
                }
              },
              "contentName": "entity.name.tag.monkyaml",
              "end": "(?x)\n                                (?=\n                                      \\s* $\n                                    | \\s+ \\#\n                                    | \\s* : (\\s|$)\n                                )\n                            ",
              "name": "string.unquoted.plain.out.monkyaml"
